    "sitedown": "^3.1.0",
    "snazzy": "^5.0.0",
    "standard": "^6.0.0",
    "tap-spec": "^4.0.2",
    "tape": "^4.0.0",
    "win-spawn": "^2.0.0"
  },
  "homepage": "https://github.com/goto-bus-stop/npm-create",
